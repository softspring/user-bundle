{
    "name": "softspring/user-bundle",
    "description": "A complete user bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "symfony/translation": "^5.4 || ^6.0",
        "javihgil/doctrine-pagination-bundle": "^2.0",
        "softspring/core-bundle": "^5.0",
        "softspring/crudl-bundle": "^5.0",
        "softspring/mime-translatable": "^5.0",
        "softspring/twig-extra-bundle": "^5.0"
    },
    "require-dev": {
        "symfony/config": "^5.4 || ^6.0",
        "symfony/console": "^5.4 || ^6.0",
        "symfony/event-dispatcher": "^5.4 || ^6.0",
        "symfony/form": "^5.4 || ^6.0",
        "symfony/framework-bundle": "^5.4 || ^6.0",
        "symfony/http-foundation": "^5.4 || ^6.0.7",
        "symfony/security-bundle": "^5.4 || ^6.0",
        "symfony/validator": "^5.4 || ^6.0",
        "symfony/yaml": "^5.4 || ^6.0",
        "friendsofphp/php-cs-fixer": "9999999-dev"
    },
    "suggest": {
        "softspring/admin-bundle": "Use softspring admin templates",
        "softspring/crudl-bundle": "Use softspring crud+list features",
        "softspring/mailer-bundle": "Integrates with softspring's mailer bundle",
        "hwi/oauth-bundle": "Integrate oauth clients"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\UserBundle\\": "src/"
        }
    },
    "extra": {
    }
}
