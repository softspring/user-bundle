{
    "name": "softspring/user-bundle",
    "description": "A complete user bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "symfony/translation": "^4.4 || ^5.3 || ^6.0",
        "softspring/core-bundle": "^4.0",
        "softspring/crudl-bundle": "^4.0",
        "softspring/mime-translatable": "^4.0"
    },
    "require-dev": {
        "symfony/config": "^4.4 || ^5.3 || ^6.0",
        "symfony/console": "^4.4 || ^5.3 || ^6.0",
        "symfony/event-dispatcher": "^4.4 || ^5.3 || ^6.0",
        "symfony/form": "^4.4 || ^5.3 || ^6.0",
        "symfony/framework-bundle": "^4.4 || ^5.3 || ^6.0",
        "symfony/http-foundation": "^4.4 || ^5.3 || ^6.0.7",
        "symfony/security-bundle": "^4.4 || ^5.3 || ^6.0",
        "symfony/validator": "^4.4 || ^5.3 || ^6.0",
        "symfony/yaml": "^4.4 || ^5.3 || ^6.0",
        "friendsofphp/php-cs-fixer": "9999999-dev"
    },
    "suggest": {
        "softspring/admin-bundle": "Use softspring admin templates",
        "softspring/crudl-bundle": "Use softspring crud+list features",
        "softspring/mailer-bundle": "Integrates with softspring's mailer bundle",
        "hwi/oauth-bundle": "Integrate oauth clients"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\UserBundle\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    }
}
