{
    "name": "softspring/user-bundle",
    "description": "A complete user bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "softspring/core-bundle": "^1.1.3",
        "softspring/crudl-bundle": "^1.0.1",
        "softspring/mailer-bundle": "^3.2.0"
    },
    "require-dev": {
        "symfony/config": "^4.2 || ^5.0",
        "symfony/console": "^4.2 || ^5.0",
        "symfony/event-dispatcher": "^4.2 || ^5.0",
        "symfony/form": "^4.2 || ^5.0",
        "symfony/framework-bundle": "^4.2 || ^5.0",
        "symfony/http-foundation": "^4.2 || ^5.0.7",
        "symfony/security-bundle": "^4.2 || ^5.0",
        "symfony/validator": "^4.2 || ^5.0",
        "symfony/yaml": "^4.2 || ^5.0"
    },
    "suggest": {
        "softspring/admin-bundle": "Use softspring admin templates",
        "softspring/crudl-bundle": "Use softspring crud+list features",
        "hwi/oauth-bundle": "Integrate oauth clients"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\UserBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2.x-dev"
        }
    }
}
