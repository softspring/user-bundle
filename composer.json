{
    "name": "softspring/user-bundle",
    "description": "A complete user bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/orm": "^2.10",
        "javihgil/doctrine-pagination-bundle": "^2.0",
        "softspring/core-bundle": "^5.0",
        "softspring/crudl-controller": "^5.0",
        "softspring/doctrine-target-entity-resolver": "^5.0",
        "softspring/events": "^5.0",
        "softspring/mime-translatable": "^5.0",
        "softspring/twig-extra-bundle": "^5.0",
        "symfony/http-foundation": "^5.4|^6.0",
        "symfony/http-kernel": "^5.4|^6.0",
        "symfony/security-bundle": "^5.4|^6.0",
        "symfony/translation": "^5.4|^6.0",
        "symfony/twig-bundle": "^5.4|^6.0",
        "symfony/validator": "^5.4|^6.0",
        "symfony/yaml": "^5.4|^6.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "fakerphp/faker": "^1.21",
        "friendsofphp/php-cs-fixer": "9999999-dev",
        "hwi/oauth-bundle": "^2.0",
        "phpstan/extension-installer": "^1.2",
        "phpstan/phpstan": "^1.9",
        "phpstan/phpstan-phpunit": "^1.2",
        "phpstan/phpstan-strict-rules": "^1.4",
        "phpstan/phpstan-symfony": "^1.2",
        "softspring/account-bundle": "^5.0",
        "softspring/doctrine-templates": "^5.0",
        "softspring/mailer-bundle": "^5.0",
        "symfony/browser-kit": "^5.4|^6.0",
        "symfony/css-selector": "^5.4|^6.0",
        "symfony/dom-crawler": "^5.4|^6.0",
        "symfony/http-client": "^5.4|^6.0",
        "symfony/mailer": "^5.4|^6.0",
        "symfony/phpunit-bridge": "^5.4|^6.0"
    },
    "suggest": {
        "softspring/admin-bundle": "Use softspring admin templates",
        "softspring/mailer-bundle": "Integrates with softspring's mailer bundle",
        "hwi/oauth-bundle": "Integrate oauth clients"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\UserBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Softspring\\UserBundle\\Tests\\TestApplication\\": "tests/TestApplication/src",
            "Softspring\\UserBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "phpstan/extension-installer": true
        }
    }
}
